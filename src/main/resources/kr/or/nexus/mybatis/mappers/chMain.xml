<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.nexus.member.chmain.dao.ChMainMapper">
 <select id="searchProjectName" resultType="String">
		SELECT
		  PROJECT_NAME
		FROM
		    PROJECT
		WHERE
		    PROJECT_ID = #{projectId}
 
 </select>

</mapper>
